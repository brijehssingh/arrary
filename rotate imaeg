#include<bits/stdc++.h>



 using namespace std; 
 
 
   void rotateImage(vector<vector<int>>&matrix){
       
       int n = matrix.size(); 
       int m = matrix[0].size();
        
         // transpose
         
          for(int i =0 ;i<n ;i++ ){
              for(int j =0 ;j<m ;j++){
                  
                  
                  swap(matrix[i][j] , matrix[j][i]); 
                  
                  
              }
          }
         
         
         // reverse 
          
                   
          for(int i =0 ;i<n ;i++ ){
            
                  
                  reverse(matrix[i].begin(), matrix[i].end());
                   
                  
                  
              
          }
       
   };
    


  int main(){
      
    int n , m ;
     
     cout<<"enter the value of row"<<endl;
      cin>>n ;
       cout<<"enter the value of column"<<endl;
        cin>>m;
         
          vector<vector<int>>matrix(n , vector<int>(m));
            
            
            for( int i =0 ;i<n ;i++){
                
                
                for(int j =0 ;j<m ;j++){
                    
                    cin>>matrix[i][j];
                    
                }
            }
    
    
    
            cout<<"before- matrix"<<endl;
            
    
       for(auto i : matrix){
           for(auto j : i){
               cout<<j<<" "; 
               
           }
           cout<<endl; 
           
       }
       
         cout<<"after- matrix"<<endl;
       
       
       
          rotateImage(matrix); 
          
       for(auto i : matrix){
           for(auto j : i){
               cout<<j<<" "; 
               
           }
           cout<<endl; 
           
       }
    
      
  }
